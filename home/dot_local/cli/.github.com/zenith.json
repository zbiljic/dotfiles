{
    "name": "zenith",
    "url": "https://github.com/bvaisvil/zenith",
    "version": "0.14.0",
    "config": {
        "type": "archive",
        "url": "{{.this.url}}/releases/download/{{.this.version}}/{{.this.name}}.{{.chezmoi.arch}}-{{.chezmoi.os}}.tgz",
        "exact": true,
        "refreshPeriod": "168h"
    },
    "remap": {
        ".chezmoi.os": {
            "darwin": "apple-darwin",
            "linux": ""
        },
        ".chezmoi.arch": {
            "amd64": "x86_64",
            "arm64": "aarch64"
        }
    }
}
