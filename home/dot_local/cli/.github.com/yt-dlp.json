{
    "name": "yt-dlp",
    "url": "https://github.com/yt-dlp/yt-dlp",
    "version": "2026.01.31",
    "config": {
        "type": "file",
        "url": "{{.this.url}}/releases/download/{{.this.version}}/{{.this.name}}_{{.chezmoi.os}}",
        "executable": true
    },
    "remap": {
        ".chezmoi.os": {
            "darwin": "macos",
            "linux": "linux"
        }
    },
    "condition": {
        ".chezmoi.config.data.ephemeral": false,
        ".chezmoi.config.data.personal": true
    }
}
