{
    "name": "rqbit",
    "url": "https://github.com/ikatson/rqbit",
    "version": "8.0.0",
    "config": {
        "type": "file",
        "url": "{{.this.url}}/releases/download/v{{.this.version}}/{{.this.name}}-{{.chezmoi.os}}-{{.chezmoi.arch}}",
        "executable": true
    },
    "remap": {
        ".chezmoi.os": {
            "darwin": "",
            "linux": "linux-static"
        },
        ".chezmoi.arch": {
            "amd64": "x86_64",
            "arm64": "aarch64"
        }
    },
    "condition": {
        ".chezmoi.config.data.personal": true
    }
}
