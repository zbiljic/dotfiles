{
    "name": "atuin",
    "url": "https://github.com/atuinsh/atuin",
    "version": "18.0.1",
    "config": {
        "type": "archive",
        "url": "{{.this.url}}/releases/download/v{{.this.version}}/{{.this.name}}-v{{.this.version}}-{{.chezmoi.arch}}-{{.chezmoi.os}}.tar.gz",
        "exact": true,
        "stripComponents": 1,
        "refreshPeriod": "168h"
    },
    "remap": {
        ".chezmoi.os": {
            "darwin": "apple-darwin",
            "linux": "unknown-linux-gnu"
        },
        ".chezmoi.arch": {
            "amd64": "x86_64",
            "arm64": "aarch64"
        }
    }
}
