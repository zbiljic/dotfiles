{
    "name": "k9s",
    "url": "https://github.com/derailed/k9s",
    "version": "0.32.4",
    "config": {
        "type": "archive",
        "url": "{{.this.url}}/releases/download/v{{.this.version}}/{{.this.name}}_{{.chezmoi.os}}_{{.chezmoi.arch}}.tar.gz",
        "exact": true,
        "refreshPeriod": "168h"
    },
    "remap": {
        ".chezmoi.os": {
            "darwin": "Darwin",
            "linux": "Linux"
        }
    },
    "condition": {
        ".chezmoi.config.data.kubernetes": true
    }
}
