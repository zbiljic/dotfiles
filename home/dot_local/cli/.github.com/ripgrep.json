{
    "name": "ripgrep",
    "url": "https://github.com/BurntSushi/ripgrep",
    "version": "14.1.0",
    "config": {
        "type": "archive",
        "url": "{{.this.url}}/releases/download/{{.this.version}}/{{.this.name}}-{{.this.version}}-{{.chezmoi.arch}}-{{.chezmoi.os}}.tar.gz",
        "exact": true,
        "stripComponents": 1,
        "refreshPeriod": "168h"
    },
    "remap": {
        ".chezmoi.os": {
            "darwin": "apple-darwin",
            "linux": "unknown-linux-musl"
        },
        ".chezmoi.arch": {
            "amd64": "x86_64",
            "arm64": ""
        }
    }
}
