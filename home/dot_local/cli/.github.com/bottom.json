{
    "name": "bottom",
    "url": "https://github.com/ClementTsang/bottom",
    "version": "0.9.3",
    "config": {
        "type": "archive",
        "url": "{{.this.url}}/releases/download/{{.this.version}}/{{.this.name}}_{{.chezmoi.arch}}-{{.chezmoi.os}}.tar.gz",
        "exact": true,
        "refreshPeriod": "168h"
    },
    "remap": {
        ".chezmoi.os": {
            "darwin": "apple-darwin",
            "linux": "unknown-linux-musl"
        },
        ".chezmoi.arch": {
            "amd64": "x86_64",
            "arm64": "aarch64"
        }
    }
}
