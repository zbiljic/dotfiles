{
    "name": "exa",
    "url": "https://github.com/ogham/exa",
    "version": "0.10.1",
    "config": {
        "type": "archive",
        "url": "{{.this.url}}/releases/download/v{{.this.version}}/{{.this.name}}-{{.chezmoi.os}}-{{.chezmoi.arch}}-v{{.this.version}}.zip",
        "exact": true,
        "stripComponents": 1,
        "refreshPeriod": "168h"
    },
    "remap": {
        ".chezmoi.os": {
            "darwin": "macos"
        },
        ".chezmoi.arch": {
            "amd64": "x86_64",
            "arm64": ""
        }
    }
}
