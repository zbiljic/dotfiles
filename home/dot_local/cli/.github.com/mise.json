{
    "name": "mise",
    "url": "https://github.com/jdx/mise",
    "version": "2024.10.8",
    "config": {
        "type": "archive",
        "url": "{{.this.url}}/releases/download/v{{.this.version}}/{{.this.name}}-v{{.this.version}}-{{.chezmoi.os}}-{{.chezmoi.arch}}.tar.gz",
        "exact": true,
        "stripComponents": 1
    },
    "remap": {
        ".chezmoi.os": {
            "darwin": "macos",
            "linux": "linux"
        },
        ".chezmoi.arch": {
            "amd64": "x64",
            "arm64": "arm64"
        }
    },
    "condition": {
        ".chezmoi.config.data.ephemeral": false
    }
}
