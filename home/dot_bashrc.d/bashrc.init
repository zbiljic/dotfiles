#!/usr/bin/env bash

# This churns through files in $HOME/.bashrc.d.
if [ -d "$HOME"/.bashrc.d ]; then
    for x in "$HOME"/.bashrc.d/* ; do
        if [[ "${x##*/}" != "bashrc.init" ]]; then
            test -f "$x" || continue
            # shellcheck source=/dev/null
            source "$x"
        fi
    done
fi
